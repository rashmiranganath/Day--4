extends layout

block content
  form(method = "post")
    p Name :
      input(type = "text",name = "name")
    p Last name :
      input(type = "text",name = "last name")
    p Address:
      input(type = "text",name = "address")
    p Dob :
      input(type = "text", name = "dob")
    p Phone number:
      input(type = "text",name = "phone number")
    p Submit :
    div.actions
      input(type = "submit", value = "add")
  